   <padding>
      <Insets bottom="10" left="10" right="10" top="10" />
   </padding>
   <children>
      <AnchorPane>
         <children>
            <Label text="Select sentence:">
               <font>
                  <Font size="20.0"/>
               </font>
            </Label>
            <ListView layoutY="30" fx:id="listSentencesView" />
         </children>
      </AnchorPane>
   </children>
</GridPane>

